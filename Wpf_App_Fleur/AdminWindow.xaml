<Page
    x:Class="Wpf_App_Fleur.AdminWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Wpf_App_Fleur"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="AdminWindow"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="White"
    ShowsNavigationUI="False"
    mc:Ignorable="d">
    <Grid>
        <TabControl>
            <TabItem Header="Accueil">
                <Grid Background="#FFE5E5E5" />

            </TabItem>
            <TabItem Header="Clients">
                <Grid Background="#FFE5E5E5">
                    <DataGridCell Margin="20,50,20,20" Background="DarkSlateGray" />
                </Grid>
            </TabItem>
            <TabItem Header="Produits">
                <Grid Background="#FFE5E5E5">
                    <DataGridCell Margin="20,50,20,20" Background="DarkSlateGray" />
                </Grid>
            </TabItem>
            <TabItem Header="Etat des Stocks">
                <Grid Background="#FFE5E5E5">
                    <DataGridCell Margin="20,50,20,20" Background="DarkSlateGray" />
                </Grid>
            </TabItem>
            <TabItem Header="Commandes à livrer">
                <Grid Background="#FFE5E5E5">
                    <DataGridCell Margin="20,50,20,20" Background="DarkSlateGray" />
                </Grid>
            </TabItem>
            <TabItem Header="Commandes en livraison">
                <Grid Background="#FFE5E5E5">
                    <DataGridCell Margin="20,50,20,20" Background="DarkSlateGray" />
                </Grid>
            </TabItem>
            <TabItem Header="Commandes livrées">
                <Grid Background="#FFE5E5E5">
                    <DataGridCell Margin="20,50,20,20" Background="DarkSlateGray" />
                </Grid>
            </TabItem>
            <TabItem Header="Déconnection">
                <Button Content="Déconnection" />
            </TabItem>
        </TabControl>
    </Grid>
</Page>
